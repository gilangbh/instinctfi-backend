{
  "name": "instinct-fi-api",
  "version": "1.0.0",
  "description": "Backend API for Instinct.fi - Gamified Consumer Trading App",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "prisma generate && tsc && tsc-alias",
    "start": "prisma migrate deploy && node dist/index.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:generate": "prisma generate",
    "db:studio": "prisma studio",
    "db:seed": "tsx src/scripts/seed.ts",
    "admin:token": "tsx src/scripts/generateAdminToken.ts",
    "postinstall": "prisma generate"
  },
  "keywords": [
    "solana",
    "trading",
    "gamification",
    "defi",
    "drift-protocol"
  ],
  "author": "Instinct.fi Team",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.30.1",
    "@drift-labs/sdk": "^2.144.0-beta.1",
    "@prisma/client": "^5.7.1",
    "@solana/spl-token": "^0.4.14",
    "@solana/web3.js": "^1.95.0",
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "big.js": "^6.2.1",
    "bs58": "^6.0.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "helmet": "^7.1.0",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "mailersend": "^2.2.0",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "redis": "^4.6.12",
    "tweetnacl": "^1.0.3",
    "uuid": "^9.0.1",
    "winston": "^3.11.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.8",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node-cron": "^3.0.11",
    "@types/supertest": "^2.0.16",
    "@types/uuid": "^9.0.7",
    "@types/ws": "^8.18.1",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.55.0",
    "jest": "^29.7.0",
    "prisma": "^5.7.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.6.2",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=20.18.0",
    "npm": ">=10.0.0"
  }
}
